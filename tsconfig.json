{
    "compilerOptions": {
        "module": "none",
        "noImplicitAny": false,
        "removeComments": true,
        "outFile": "./dist/index.js",
        "types": [
            "./typings"
        ],
        "sourceMap": false,
        "declaration": true
    },
    "include": [
		"src/**/*",
		"typings/**/*"
    ],
    "exclude": [
		"node_modules",
		"dist/**/*"
    ]
}
